@model IEnumerable<pweb_eCarSharing.Models.CarStation>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.stationCity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.stationAdress)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.stationCity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.stationAdress)
        </td>
    </tr>
}

</table>
